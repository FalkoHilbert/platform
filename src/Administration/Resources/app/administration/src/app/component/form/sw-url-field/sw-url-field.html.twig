{% block sw_url_field %}
<mt-url-field
    v-if="!deprecated"
    v-bind="$attrs"
    v-model="realValue"
>
    <template
        v-for="(index, name) in getSlots()"
        #[name]="data"
    >
        <slot
            :name="name"
            v-bind="data || {}"
        >
        </slot>
    </template>
</mt-url-field>
{% endblock %}

{# Deprecated component #}
<sw-url-field-deprecated
    v-else
    v-bind="$attrs"
    v-model:value="realValue"
>
    <template
        v-for="(_index, name) in getSlots()"
        #[name]="data"
    >
        <slot
            :name="name"
            v-bind="data || {}"
        >
        </slot>
    </template>
</sw-url-field-deprecated>
